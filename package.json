{
  "name": "extend-decorator",
  "version": "0.0.1",
  "description": "Extend your objects using a decorator!",
  "main": "dist/extend-decorator.js",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "babili src/ -d dist/",
    "pretest": "eslint src/ && npm run build",
    "test": "ava",
    "example": "cd example/ && webpack && http-server dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bbmoz/extend-decorator.git"
  },
  "keywords": [
    "es6",
    "es7",
    "es2015",
    "es2016",
    "babel",
    "decorators",
    "annotations",
    "inject",
    "extend"
  ],
  "author": "Joe Lee <https://github.com/bbmoz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbmoz/extend-decorator/issues"
  },
  "homepage": "https://github.com/bbmoz/extend-decorator",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "dist/**/*.js"
    ],
    "failWithoutAssertions": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "env",
        "latest"
      ]
    }
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.3.2",
    "babel-preset-latest": "^6.24.0",
    "babili": "0.0.12",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "html-webpack-plugin": "^2.28.0",
    "webpack": "^2.3.2"
  }
}
